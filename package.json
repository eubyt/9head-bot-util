{
    "name": "bot_d9",
    "version": "0.0.1",
    "description": "Um bot de moderação para o servidor D9",
    "main": "./dist/start-bot.js",
    "author": "eubyt",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=20.X"
    },
    "devDependencies": {
        "@eslint/js": "^9.31.0",
        "@flydotio/dockerfile": "^0.7.10",
        "@types/node": "^24.1.0",
        "eslint": "^9.31.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-unicorn": "^60.0.0",
        "globals": "^16.3.0",
        "prettier": "^3.6.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.38.0"
    },
    "scripts": {
        "clear:dist": "if [ -d \"dist\" ]; then rm -R dist; fi",
        "build": "yarn run clear:dist && tsc --build",
        "start:bot": "node --enable-source-maps dist/start-bot.js",
        "start": "yarn run start:bot",
        "dev": "yarn run build && yarn run start:bot"
    },
    "dependencies": {
        "@twurple/api": "^7.3.0",
        "@twurple/auth": "^7.3.0",
        "@twurple/chat": "^7.3.0",
        "@twurple/eventsub-ws": "^7.3.0",
        "@types/tmi.js": "^1.8.6",
        "discord.js": "^14.21.0",
        "dotenv": "^17.2.0",
        "firebase-admin": "^13.4.0"
    }
}
